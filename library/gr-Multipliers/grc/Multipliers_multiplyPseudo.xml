<?xml version="1.0"?>
<block>
  <name>multiplyPseudo</name>
  <key>Multipliers_multiplyPseudo</key>
  <category>[Multipliers]</category>
  <import>import Multipliers</import>
  <make>Multipliers.multiplyPseudo($chips, $tam_info)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
   <param>
     <name>Chips</name>
     <key>chips</key>
     <type>int</type>
   </param>

   <param>
     <name>Data Size</name>
     <key>tam_info</key>
     <type>int</type>
   </param>

   <!-- Make one 'sink' node per input. Sub-nodes:
        * name (an identifier for the GUI)
        * type
        * vlen
        * optional (set to 1 for optional inputs) -->
   <sink>
     <name>Data</name>
     <type>float</type>
   </sink>

   <sink>
     <name>PN</name>
     <type>float</type>
   </sink>

   <!-- Make one 'source' node per output. Sub-nodes:
        * name (an identifier for the GUI)
        * type
        * vlen
        * optional (set to 1 for optional inputs) -->
   <source>
     <name>out</name>
     <type>float</type>
   </source>
</block>
